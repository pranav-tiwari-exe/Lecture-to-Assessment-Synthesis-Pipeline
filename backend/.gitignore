*.bak
*.backup
*.old

# Editor directories and files
.vscode/*
!.vscode/extensions.json
.idea
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw?

# PM2 process files
.pm2/
ecosystem.config.js

# MongoDB data (if running locally)
data/
mongodb/
